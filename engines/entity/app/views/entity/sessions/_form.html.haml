= simple_form_for @session, remote: true do |f|
  = f.input :cnpj, as: :integer, label: false, placeholder: 'Informe seu CNPJ'
  = f.input :password, label: false, placeholder: 'Informe sua senha'
  = f.submit 'Realizar o acesso', class: 'btn btn-primary btn-block', data: { disable_with: t(:disable_with)}
  = link_to "NÃ£o consigo acessar", entity.new_remember_path, class: 'btn btn-dark btn-block'